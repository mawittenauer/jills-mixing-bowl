<% provide(:page, "gallery") %>
<ul id="gallery">
  <% @cakes.each do |cake| %>
    <li>
      <%= image_tag(cake.cake_url, class: "gallery-image") %>
      <% if logged_in? %>
        <%= link_to "Delete", cake_path(cake), method: :delete %>
      <% end %>
    </li>
  <% end %>
</ul>

<% if logged_in? %>
  <%= link_to "New Cake", new_cake_path, class: "button" %>
<% end %>
